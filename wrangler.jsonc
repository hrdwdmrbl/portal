{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "portlsignal",
  "compatibility_date": "2025-01-06",
  "main": "worker/index.ts",
  "routes": [
    {
      "pattern": "portl.cam",
      "custom_domain": true
    }
  ],
  "assets": {
    "directory": "./public/"
  },
  "observability": {
    "logs": {
      "enabled": true
    }
  },
  "durable_objects": {
    "bindings": [
      {
        "name": "PORTAL_ROOM",
        "class_name": "PortalManager"
      }
    ]
  }
}
